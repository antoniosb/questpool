<% if user_signed_in? %>
<li>
  <a href="<%= destroy_user_session_path %>" class="tooltipped" data-position="bottom" data-delay="50" data-tooltip="Sair">
    <i class="material-icons">exit_to_app</i>
  </a>
</li>
<% else %>
<li>
  <a href="  <%= new_user_session_path %>" class="tooltipped" data-position="bottom" data-delay="50" data-tooltip="Entrar">
    <i class="material-icons">exit_to_app</i>
  </a>
</li>
<% end %>
